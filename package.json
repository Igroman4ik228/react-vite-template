{
	"name": "react-vite-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"tsc": "tsc",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier . -c -u --cache",
		"format:fix": "prettier . -w -u --cache"
	},
	"lint-staged": {
		"*.{ts,tsx}": "bash -c 'tsc -p tsconfig.app.json'",
		"**/*.{js,jsx,ts,tsx}": "eslint --fix",
		"*": "prettier -w -u --log-level error"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.9.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@types/node": "^24.6.0",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.4",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.36.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.45.0",
		"vite": "^7.1.7"
	}
}
